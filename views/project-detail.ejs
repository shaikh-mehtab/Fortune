  </section>

  <section
    class="bg-dark-gray background-position-center-top overlap-height overflow-visible"
    style="
      background-image: url('/images/demo-architecture-dotted-pattern.svg');
    "
  >
    <div class="container">
      <div class="row g-0">
        <div class="col bg-very-light-gray p-35px lg-ps-15px lg-pe-15px border-radius-6px fs-16">
        <div class="row row-cols-2 row-cols-md-4 row-cols-sm-2">
          <% amenities.forEach((amn,i)=>{ %>
        <div class="col text-center border-end border-color-extra-medium-gray sm-mb-30px">
        <img src="/api/transform/<%= JSON.parse(amn.icon)[0] %>" class="w-50px mb-15px" alt="" data-no-retina="">
        <span class="text-dark-gray d-block lh-20"><%= amn.name%></span>
        </div>
          <% }) %>
        </div>
        </div>
        </div>
      <div class="row overlap-gap-section">
        <div class="col-lg-6 col-md-6 text-center text-md-start">
          <h4 class="fw-600 text-white">
            <!-- <%= project.title.slice(0,5) %> -->
            <span class="text-decoration-line-bottom d-inline-block"
              ><%= project.title.slice(0) %></span
            >
          </h4>
          <p class="mb-35px">
            Embrace a lifestyle of modern luxury with our meticulously designed
            amenities, crafted to provide unparalleled comfort and
            sophistication. Experience top-notch facilities that blend elegance
            with functionality, reflecting our commitment to quality and
            excellence.Enjoy superior craftsmanship and cutting-edge design that
            cater to your every need and desire.
          </p>
          <button
            class="btn btn-small btn-base-color btn-box-shadow btn-round-edge-small primary-font fw-700 submit"
            onclick="openPdf()"
          >
            Get Brochure
          </button>
          <!-- <a
          href="#"
          class="btn btn-medium btn-base-color text-uppercase-inherit fw-600 primary-font"
          >Yourdomain.com</a
        > -->
        </div>
      </div>
    </div>
  </section>

  <section
    class="bg-dark-gray background-position-center-top position-relative"
    style="background-image: url('images/demo-architecture-dotted-pattern.svg')"
    data-anime='{ "el": "childs", "translateY": [30, 0], "opacity": [0,1], "duration": 500, "delay": 200, "staggervalue": 300, "easing": "easeOutQuad" }'
  >
    <div class="container">
      <div
        class="row justify-content-center overlap-section overlap-section-three-fourth mb-6"
      >
        <div class="col-lg-10">
          <div class="p-9 sm-p-7 bg-nero-grey">
            <form
              action="/api/createenquiries"
              method="post"
              class="contact-form-style-03"
            >
              <div class="row justify-content-center">
                <div class="col-md-9 mb-7 xs-mb-30px">
                  <h4 class="text-white fw-600 mb-0">
                    <span class="text-base-color">Let's talk.</span> Have a
                    Question in mind?
                  </h4>
                </div>
                <div class="col-md-3 text-end d-none d-md-inline-block">
                  <img
                    src="/api/transform/<%=JSON.parse(footerData.store_setting[0].logo1) %>"
                    class="h-50px"
                    alt
                  />
                </div>
                <div class="col-md-6">
                  <label
                    for="exampleInputEmail1"
                    class="form-label fs-11 ls-2px text-uppercase text-white mb-0"
                    >Your name*</label
                  >
                  <div class="position-relative form-group mb-30px">
                    <span class="form-icon"
                      ><i class="bi bi-emoji-smile"></i
                    ></span>
                    <input
                      class="fs-15 ps-0 border-radius-0px border-color-charcoal-grey bg-transparent form-control required"
                      id="exampleInputEmail1"
                      type="text"
                      name="name"
                      placeholder="What's your good name?"
                    />
                  </div>
                </div>
                <div class="col-md-6">
                  <label
                    for="exampleInputEmail1"
                    class="form-label fs-11 ls-2px text-uppercase text-white mb-0"
                    >Your phone number</label
                  >
                  <div class="position-relative form-group mb-30px">
                    <span class="form-icon"
                      ><i class="bi bi-telephone"></i
                    ></span>
                    <input
                      class="fs-15 ps-0 border-radius-0px border-color-charcoal-grey bg-transparent form-control"
                      id="exampleInputEmail3"
                      type="tel"
                      name="phone_no"
                      placeholder="Enter your phone number"
                    />
                  </div>
                </div>
                <div class="col-md-12">
                  <label
                    for="exampleInputEmail1"
                    class="form-label fs-11 ls-2px text-uppercase text-white mb-0"
                    >Your email address*</label
                  >
                  <div class="position-relative form-group mb-30px">
                    <span class="form-icon"
                      ><i class="bi bi-envelope"></i
                    ></span>
                    <input
                      class="fs-15 ps-0 border-radius-0px border-color-charcoal-grey bg-transparent form-control required"
                      id="exampleInputEmail2"
                      type="email"
                      name="email"
                      placeholder="Enter your email address"
                    />
                  </div>
                </div>
                <div class="col-12 mb-5">
                  <label
                    for="exampleInputEmail1"
                    class="form-label fs-11 ls-2px text-uppercase text-white mb-0"
                    >Your message</label
                  >
                  <div class="position-relative form-group form-textarea mb-0">
                    <textarea
                      class="fs-15 ps-0 border-radius-0px border-color-charcoal-grey bg-transparent form-control"
                      name="message"
                      placeholder="How can we help you?"
                      rows="4"
                    ></textarea>
                    <span class="form-icon"
                      ><i class="bi bi-chat-square-dots"></i
                    ></span>
                  </div>
                </div>
                <div
                  class="col-xl-7 col-md-8 text-center text-md-start sm-mb-25px"
                >
                  <p class="mb-0 fs-14 lh-24 w-90 xl-w-100">
                    We are committed to protecting your privacy. We will never
                    collect information about you without your explicit consent.
                  </p>
                </div>
                <div class="col-xl-5 col-md-4 text-center text-md-end">
                  <input
                    id="exampleInputEmail5"
                    type="hidden"
                    name="project_id"
                    value="<%= project.project_id %>"
                  />
                  <input
                    id="exampleInputEmail5"
                    type="hidden"
                    name="form_location"
                    value="Projects"
                  />
                  <button
                    class="btn btn-small btn-base-color btn-box-shadow btn-round-edge-small primary-font fw-700 submit"
                    type="submit"
                  >
                    Send message
                  </button>
                </div>
                <div class="col-12">
                  <div class="form-results mt-20px d-none"></div>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
    <div
      class="position-absolute bottom-minus-50px lg-bottom-minus-40px md-bottom-minus-25px sm-bottom-minus-20px xs-bottom-minus-10px left-0px right-0px text-center w-100 fs-200 lg-fs-160 md-fs-140 sm-fs-120 xs-fs-90 fw-600 text-nero-grey ls-minus-4px"
    >
      architecture
    </div>
  </section>

  <div
    class="one-half-screen md-small-screen sm-extra-small-screen cover-background"
    style="
      background-image: url('/images/demo-architecture-dotted-pattern.svg');
    "
  >
    <iframe
      src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d14961.608814065663!2d72.91057698006598!3d20.366300078750275!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3be0ce47d36ac1cf%3A0x160e276063e27a55!2sFORTUNE%20LANDMARK%20BY%20FORTUNE%20DREAM-CON%20PVT.%20LTD.!5e0!3m2!1sen!2sin!4v1717580931559!5m2!1sen!2sin"
      width="1500"
      height="600"
      style="border: 0"
      allowfullscreen="true"
      loading="lazy"
      referrerpolicy="no-referrer-when-downgrade"
    ></iframe>
  </div>
  <script>
    function openPdf() {
      window.open(`/<%= project.brochure %>`, "_blank");
    }
  </script>
  <%- include('include/footer') %>
</body>
